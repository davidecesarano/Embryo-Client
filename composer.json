{
    "name": "davidecesarano/embryo-client",
    "type": "library",
    "description": "Simple PSR-18 HTTP Client implementation.",
    "keywords": [
        "php",
        "psr-18",
        "http-client"
    ],
    "homepage": "https://github.com/davidecesarano/embryo-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Cesarano",
            "homepage": "https://github.com/davidecesarano"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/http-message": "1.0.1",
        "psr/http-client": "1.0.0",
        "davidecesarano/embryo-http": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Embryo\\": "Embryo/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^0.12.58"
    }
}
